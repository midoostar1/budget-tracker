{
  "name": "@apps/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "@aws-sdk/s3-presigned-post": "^3.665.0",
    "@bull-board/api": "^6.0.7",
    "@bull-board/express": "^6.0.7",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "^5.21.1",
    "firebase-admin": "^12.7.0",
    "bullmq": "^5.12.13",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "morgan": "^1.10.0",
    "zod": "^3.23.8",
    "zod-to-openapi": "^7.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.16.11",
    "@types/morgan": "^1.9.9",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "type": "module"
}