{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowJs": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["react", "react-native"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src", "App.tsx"]
}
